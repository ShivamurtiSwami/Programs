class Main {

  public static int FirstFactorial(int num) {
    // code goes here  
    int i,fact=1;
    for(i=1; i<=num; i++){
      fact*=i;
    }
    return fact;
  }

  public static void main (String[] args) {  
    // keep this function call here     
    Scanner s = new Scanner(System.in);
    System.out.print(FirstFactorial(s.nextLine())); 
  }

}
